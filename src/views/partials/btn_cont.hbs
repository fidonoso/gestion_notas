<!---form Contabilidad----->
<form class="pt-2 pb-2 ms-3" action="/docente/contabilidad" method="post" id="form3">
    <div class="form-check form-switch">
      {{#if habilitado_cont}}
        {{#if estp_car_cont}}
        <input class="form-check-input" type="checkbox" checked value="Contabilidad" name="checksu" id="check_3">
        <label class="form-check-label" for="checkContabilidad" id="lbl_3">Curso abierto</label>
        {{else}}
        <input class="form-check-input" type="checkbox" value="Contabilidad" name="checksu" id="check_3">
        <label class="form-check-label" for="checkContabilidad" id="lbl_3">Curso cerrado</label>
        {{/if}}
      {{else}}
      <label class="form-check-label" for="checkContabilidad" id="lbl_3">Finalizado por el supervisor</label>
      {{/if}}
      <input class="form-control d-none" type="text" value="Contabilidad" id="contabilidad" name="carrera">
    </div>
    <button class="btn btn-{{#if btnCon}}{{btnCon}}{{else}}secondary{{/if}}" type="submit" >Contabilidad</button>
</form>